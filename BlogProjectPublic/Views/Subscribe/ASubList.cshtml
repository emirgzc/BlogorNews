
@model IEnumerable<EntityLayer.Concrete.Subscribe>

@{
    ViewBag.Title = "ASubList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


<div class="col-lg-12">
    <div class="card">
        <div class="card-body">

            <div class="card-title">
                <h4>Table Basic</h4>
            </div>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Email</th>
                            <th>Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <th>#</th>
                                <td>@item.Email</td>
                                <td>@item.Date.ToString("d MMMMM yyy")</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


