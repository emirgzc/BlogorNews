
@model IEnumerable<EntityLayer.Concrete.Settings>

@{
    ViewBag.Title = "ASetList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="col-lg-12">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Site Aayarları</h4>
            <div class="table-responsive">
                <table class="table header-border">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Telefon</th>
                            <th>Mail</th>
                            <th>Yönetici</th>
                            <th>Güncelle</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>#</td>
                                <td>@item.Phone</td>
                                <td>@item.Email</td>
                                <td>@item.DirectorName</td>
                                <td><a href="/Settings/ASettingUpdate/@item.Id" class="btn btn-success fa fa-check"></a></td>
                            </tr>

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

