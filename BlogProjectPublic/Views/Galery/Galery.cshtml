
@model IEnumerable<EntityLayer.Concrete.Galery>

<section class="section instagram-feed">
    <div class="container">
        <div class="row">
            <div class="title">
                <h2>Galery</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                @foreach (var item in Model)
                {
                    <div class="col-md-3">
                        <a href="@item.Image" target="_blank"><img class="img-responsive" src="@item.Image" /></a>
                    </div>
                }
            </div>
        </div>
    </div>
</section>
